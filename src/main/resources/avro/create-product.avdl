@namespace("com.sarya.graphql.service")
protocol AvroSchemaTool {
	record CreatProduct {
		@logicalType("UUID")
		string productId;
		string name;
		ProductType productType;
		ProductStatus productStatus;
		boolean active;
	}

	enum ProductType {
		BIKE,
		SCOOTER
	}

	enum ProductStatus {
		AVAILABLE,
		RENTED
	}
}